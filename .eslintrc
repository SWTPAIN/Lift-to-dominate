{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "spaced-comment": 0,
    "new-cap": [1, { "capIsNewExceptions": ["Map", "Record", "List"] }],
    "react/prefer-stateless-function": 1,
    "react/sort-comp": 0,
    "object-curly-spacing": 0,
    "react/jsx-no-bind": 1,
    "no-shadow": 1,
  },
  "plugins": ["react"]
}
